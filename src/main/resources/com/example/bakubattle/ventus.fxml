<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1080.0" maxWidth="1920.0" prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bakubattle.VentusBakuController">
    <children>
        <!--      TODO-->
        <ImageView fx:id="ventus_baku_view" fitHeight="1306.0" fitWidth="2278.0" layoutX="-24.0" layoutY="-14.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@arena/bakugan/Auxillataur_Ventus_Details.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="-14.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchLeft.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="1606.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchRight.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="400.0" fitWidth="900.0" layoutX="584.0" layoutY="902.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/Ventus_frame.png" />
            </image>
        </ImageView>
        <Label fx:id="ventus_name_label" alignment="CENTER" layoutX="732.0" layoutY="930.0" prefHeight="194.0" prefWidth="632.0" text="Auxillataur" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Papyrus" size="120.0" />
            </font>
        </Label>
        <Button fx:id="ventus_left_switch_button" layoutX="114.0" layoutY="541.0" mnemonicParsing="false" onAction="#goLeft" opacity="0" prefHeight="171.0" prefWidth="130.0" text="Button">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="ventus_right_switch_button" layoutX="1735.0" layoutY="542.0" mnemonicParsing="false" onAction="#goRight" opacity="0" prefHeight="171.0" prefWidth="130.0" text="Button">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="ventus_choice_of_bakugan" layoutX="616.0" layoutY="938.0" mnemonicParsing="false" onAction="#selected" opacity="0" prefHeight="235.0" prefWidth="820.0" text="Button">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
