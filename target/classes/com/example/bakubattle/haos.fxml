<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1080.0" maxWidth="1920.0" minHeight="1080.0" minWidth="1920.0" prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bakubattle.HaosBakuController">
    <children>
        <!--      TODO-->
        <ImageView fx:id="haos_baku_view" fitHeight="1101.0" fitWidth="1948.0" layoutX="-24.0" layoutY="-14.0" opacity="0.9" pickOnBounds="true">
            <image>
                <Image url="@arena/bakugan/Hydranoid_Haos_Details.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="-28.0" layoutY="361.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchLeft.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="1572.0" layoutY="366.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchRight.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="400.0" fitWidth="900.0" layoutX="492.0" layoutY="772.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/Haos_frame.png" />
            </image>
        </ImageView>
        <Label fx:id="haos_name_label" alignment="CENTER" layoutX="640.0" layoutY="800.0" prefHeight="194.0" prefWidth="632.0" text="Hydranoid" textAlignment="CENTER" textFill="WHITE">
