                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="haos_right_switch_button" layoutX="1701.0" layoutY="508.0" mnemonicParsing="false" onAction="#goRight" opacity="0" prefHeight="171.0" prefWidth="130.0" text="Button">
