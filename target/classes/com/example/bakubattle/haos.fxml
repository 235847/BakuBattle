                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="haos_choice_of_bakugan" layoutX="524.0" layoutY="808.0" mnemonicParsing="false" onAction="#selected" opacity="0" prefHeight="235.0" prefWidth="820.0" text="Button">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
