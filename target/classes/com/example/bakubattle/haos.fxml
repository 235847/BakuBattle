                <Font name="Papyrus" size="120.0" />
            </font>
        </Label>
        <Button fx:id="haos_left_switch_button" layoutX="100.0" layoutY="502.0" mnemonicParsing="false" onAction="#goLeft" opacity="0" prefHeight="171.0" prefWidth="130.0" text="Button">
