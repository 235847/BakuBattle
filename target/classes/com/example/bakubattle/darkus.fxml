<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1080.0" maxWidth="1920.0" minHeight="1080.0" minWidth="1920.0" prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bakubattle.DarkusBakuController">
    <children>
        <!--      TODO-->
        <ImageView fx:id="darkus_baku_view" fitHeight="1100.0" fitWidth="1952.0" layoutX="-24.0" layoutY="-14.0" opacity="0.9" pickOnBounds="true">
            <image>
                <Image url="@arena/bakugan/Dragonoid_Darkus_Details.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="-64.0" layoutY="372.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchLeft.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="542.0" fitWidth="390.0" layoutX="1596.0" layoutY="365.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/SwitchRight.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="400.0" fitWidth="900.0" layoutX="488.0" layoutY="744.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@bakugan_select/Darkus_frame.png" />
            </image>
        </ImageView>
        <Label fx:id="darkus_name_label" alignment="CENTER" layoutX="636.0" layoutY="772.0" prefHeight="194.0" prefWidth="632.0" text="Dragonoid" textAlignment="CENTER" textFill="WHITE">
